<%= link_to "New attachment", new_attachment_url %>
<table>
  <tr><th>Id</th><th>Filepicker.io Url</th><th>image</th></tr>

  <% @attachments.each do |a| %>

  <tr><td><%= a.id %></td><td><%= a.file_url %></td>
    <td><%= filepicker_image_tag a.file_url, w: 300, h: 300 %></td>
  <td><%= filepicker_save_button "Download", a.file_url, "image/png" %></tr>

  <% end %>
</table>
